<h1>Profiles#edit</h1>


<%= form_with model: @profile do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :rating %>
        <%= f.text_field :rating %>
    </p>
    <p>
        <%= f.label :owner %>
        <%= f.text_field :owner %>
    </p>
    <p>
        <%= f.submit 'Update profile' %>
    </p>
<% end %>

